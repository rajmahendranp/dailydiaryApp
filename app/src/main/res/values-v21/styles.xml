<!--        <item name="android:calendarViewShown">false</item>-->
<!--        <item name="android:datePickerMode"></item>-->
<!--    </style>-->
    <style name="DatePicker" parent="ThemeOverlay.AppCompat.Dialog">
        <item name="colorAccent">@color/colorPrimary</item>                   <!--header background-->
        <item name="android:windowBackground">@color/black</item>     <!--calendar background-->
        <item name="android:colorControlActivated">@color/gray</item> <!--selected day-->
        <item name="android:textColorPrimary">@color/colorPrimary</item> <!--days of the month-->
        <item name="android:textColorSecondary">@color/colorPrimary</item>    <!--days of the week-->
        <item name="android:datePickerStyle">@android:style/Widget.DatePicker</item>
    </style>
    <style name="DatePicker1" parent="ThemeOverlay.AppCompat.Dialog">
        <item name="colorAccent">@color/backblue</item>                   <!--header background-->
        <item name="android:windowBackground">@color/White</item>     <!--calendar background-->
        <item name="android:colorControlActivated">@color/gray</item> <!--selected day-->
        <item name="android:textColorPrimary">@color/backblue</item> <!--days of the month-->
        <item name="android:textColorSecondary">@color/backblue</item>    <!--days of the week-->
        <item name="android:datePickerStyle">@android:style/Widget.DatePicker</item>
    </style>
    <style name="DialogTheme1" parent="ThemeOverlay.AppCompat.Dialog">
        <item name="colorAccent">@color/backblue</item>                   <!--header background-->
        <item name="android:windowBackground">@color/White</item>     <!--calendar background-->
        <item name="android:colorControlActivated">@color/gray</item> <!--selected day-->
        <item name="android:textColorPrimary">@color/backblue</item> <!--days of the month-->
        <item name="android:textColorSecondary">@color/backblue</item>    <!--days of the week-->
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowNoTitle">true</item>
    </style>
    <style name="AppTheme.NoActionBar1">
        <item name="android:background">@color/White</item>
        <item name="android:textColor">@color/Black</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="AppTheme.NoActionBar2">
        <item name="android:background">@color/Black</item>
        <item name="android:textColor">@color/White</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="AppTheme.NoActionBar3">
        <item name="android:background">@color/Orangee</item>
        <item name="android:textColor">@color/White</item>
        <item name="android:textColorSecondary">@color/White</item>
        <item name="android:tint">#62ff00</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
    <style name="checkBoxStyle" parent="Base.Theme.AppCompat">
        <item name="colorAccent">@color/Orangee</item>
        <item name="popupTheme">@style/ThemeOverlay.AppCompat.Light</item>
        <item name="colorPrimary">@color/colorPrimary</item>

        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="android:textColorPrimary">@color/White</item>
    </style>
    <style name="TextLabel" parent="TextAppearance.AppCompat">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:textSize">20dp</item>
        <item name="android:textColorHint">@color/Gray</item>
        <item name="colorAccent">@color/Gray</item>
        <item name="colorControlNormal">@color/Gray</item>
    </style>
    <style name="RobotoTextViewStyle" parent="android:Widget.TextView">
        <item name="android:fontFamily" tools:targetApi="jelly_bean">@font/opensans</item>
    </style>
</resources>
